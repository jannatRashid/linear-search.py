f="no"
a=[23,42,96,68,79,43,22,99,66,86,77]
print(a)
n=len(a)
value=int(input("enter your required value:"))
i=0
while(i<n):
    if(a[i]==value):
        print(a[i]==value)
        print("your required value is at position ",i+1,"\n",end=" ")
        f="yes"
    i+=1
if (f=="no"):
    print("desired value not found")
